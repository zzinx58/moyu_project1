generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DEV_DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model tournament_apply_t {
  id            Int     @id @default(autoincrement())
  t_id          Int
  p_id          Int
  p_name        String  @db.VarChar(255)
  user_id       Int
  name          String  @db.VarChar(255)
  identity_card String? @db.VarChar(255)
  tel           String? @db.VarChar(255)
  gender        String? @db.VarChar(255)
  region_id     Int?
  region        String? @db.VarChar(255)
  wca_id        String? @db.VarChar(255)
  apply_time    BigInt?
  verify        Int?    @db.TinyInt
  verify_time   BigInt?
  t_number      Int?

  @@index([identity_card], map: "identity_card")
  @@index([p_id], map: "p_id")
  @@index([t_id], map: "t_id")
  @@index([user_id], map: "user_id")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model tournament_result_t {
  id            BigInt  @id @default(autoincrement())
  t_id          Int
  p_id          Int
  p_name        String  @db.VarChar(255)
  user_id       Int
  name          String  @db.VarChar(255)
  phase         Int     @db.TinyInt
  r1_duration   Int
  r1_scramble   String? @db.VarChar(1024)
  r2_duration   Int
  r2_scramble   String? @db.VarChar(1024)
  r3_duration   Int
  r3_scramble   String? @db.VarChar(1024)
  r4_duration   Int
  r4_scramble   String? @db.VarChar(1024)
  r5_duration   Int
  r5_scramble   String? @db.VarChar(1024)
  best_duration Int
  avg           Int
  is_rise       Int?    @default(0) @db.TinyInt
  record_id     Int?    @default(0)
  cube_id       String? @db.VarChar(255)
  create_time   BigInt? @default(0)
  del_time      BigInt? @default(0)
  result        Int?    @default(0)
  is_del        Int?    @db.TinyInt
  t_number      Int?

  @@index([p_id], map: "p_id")
  @@index([phase], map: "phase")
  @@index([t_id], map: "t_id")
  @@index([user_id], map: "user_id")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model tournament_room_t {
  id         BigInt @id
  t_id       Int
  p_id       Int
  phase      Int    @db.TinyInt
  room_quota Int    @db.MediumInt
  room_limit Int    @db.MediumInt
  is_start   Int    @db.TinyInt
  time_1     BigInt
  time_2     BigInt
  rule_type  Int    @db.TinyInt
  room_list  String @db.Text
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model tournament_schedule_t {
  id              BigInt @id
  t_id            Int
  p_id            Int
  p_name          String @db.VarChar(255)
  seq             Int    @db.TinyInt
  seq_time        String @db.VarChar(255)
  seq_name        String @db.VarChar(255)
  seq_icon        String @db.VarChar(255)
  phase           Int    @db.TinyInt
  rule_type       Int    @db.TinyInt
  pass_line       Int
  reduction_limit Int

  @@index([p_id], map: "p_id")
  @@index([t_id], map: "t_id")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model tournament_t {
  id                      BigInt   @id @default(autoincrement()) @db.UnsignedBigInt
  type                    String?  @db.VarChar(255)
  on_off_line             Int      @db.TinyInt
  name                    String?  @db.VarChar(255)
  sub_name                String?  @db.VarChar(255)
  apply_time_1            BigInt?
  apply_time_2            BigInt?
  r1_time_1               BigInt?
  r1_time_2               BigInt?
  r2_time_1               BigInt?
  r2_time_2               BigInt?
  r3_time_1               BigInt?
  r3_time_2               BigInt?
  close_time              BigInt?
  organizer               String?  @db.VarChar(255)
  location                String?  @db.VarChar(255)
  apply_quota             Int?     @db.MediumInt
  r1_quota                Int?     @db.MediumInt
  r1_rule                 String?  @db.VarChar(1024)
  r1_rule_type            Int?     @db.TinyInt
  r1_pass_line            Int?     @db.MediumInt
  r2_quota                Int?     @db.MediumInt
  r2_rule                 String?  @db.VarChar(1024)
  r2_rule_type            Int?     @db.TinyInt
  r2_pass_line            Int?     @db.MediumInt
  r3_rule                 String?  @db.VarChar(1024)
  r3_rule_type            Int?     @db.TinyInt
  room_quota              Int?     @db.MediumInt
  room_limit              Int?     @db.MediumInt
  rule                    String?  @db.VarChar(1024)
  organizer_web_url       String?  @db.VarChar(255)
  groups                  String?  @db.VarChar(255)
  apply_condition         String?  @db.VarChar(255)
  apply_condition_remarks String?  @db.VarChar(255)
  published               Boolean? @default(true)
  apply_time_range        Json?
  time_range              Json?
  projects_detail         Json?
  display_order           Int?
  display_on_homepage     Boolean?
  banner                  Json?
  type_id                 Int?
  apply_web_url           String?  @db.VarChar(255)
  apply_image_url         String?  @db.VarChar(255)
  audit_status            Int?     @default(2)
  award_time_range        Json?
  signin_time_range       Json?
  break_time              Json?
  lottery_time            Json?
  projects                String?  @db.VarChar(255)
  status_id               Int?     @db.TinyInt
}
